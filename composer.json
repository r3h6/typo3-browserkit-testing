{
    "name": "r3h6/typo3-webtesting",
    "description": "Webtests for TYPO3",
    "type": "library",
    "require": {
        "symfony/browser-kit": "^5.4",
        "typo3/testing-framework": "^6.16"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "R3H6\\Typo3Webtesting\\": "src/"
        }
    },
    "autoload-dev": {
		"psr-4": {
			"R3H6\\Typo3Webtesting\\Tests\\": "tests/"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/public"
		}
	},
    "require-dev": {
        "typo3/cms-fluid-styled-content": "^11.5"
    }
}
